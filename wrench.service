[Unit]
Description=wrench daemon script
After=auditd.service systemd-user-sessions.service time-sync.target

[Service]
User=gregory
TimeoutStartSec=0
Type=simple
KillMode=process
WorkingDirectory=/tmp
ExecStart=/home/gregory/go/other/bin/wrench -o /tmp/wrench -s
Restart=always
RestartSec=2
LimitNOFILE=5555

[Install]
WantedBy=multi-user.target
